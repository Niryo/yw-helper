{
  "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noEmit": true,
      "allowJs": true,
      "useUnknownInCatchVariables": false,
  },
  "exclude": ["node_modules"]
}
